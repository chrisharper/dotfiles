Host *
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_ed25519

Host debian
  Hostname localhost
  User dev
  Port 2222
  ForwardAgent yes
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no

Host fedora
  Hostname localhost
  User dev
  Port 3333
  ForwardAgent yes
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no

Host arch
  Hostname localhost
  User dev
  Port 4444 
  ForwardAgent yes
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
